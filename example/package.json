{
  "name": "estrattonbailey",
  "version": "0.0.1",
  "main": "public/index.js",
  "scripts": {
    "dev": "concurrently 'npm run dev:js' 'npm run dev:css'",
    "build:js": "NODE_ENV=production webpack -p",
    "build:css": "NODE_ENV=production postcss -c postcss.config.js",
    "dev:js": "NODE_ENV=development webpack -d --watch",
    "dev:css": "NODE_ENV=development postcss -w -c postcss.config.js",
    "lint": "eslint src/js/**/*.js",
    "deploy": "cd dist && now -e NODE_ENV=production"
  },
  "dependencies": {},
  "devDependencies": {
    "autoprefixer": "^6.5.3",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.0",
    "babel-loader": "^6.2.7",
    "babel-plugin-lodash": "^3.2.10",
    "babel-plugin-module-resolver": "^2.4.0",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-node6": "^11.0.0",
    "babel-preset-react": "^6.16.0",
    "concurrently": "^3.1.0",
    "cssnano": "^3.8.2",
    "eslint": "^3.12.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-react": "^6.8.0",
    "eslint-plugin-standard": "^2.0.1",
    "fab.js": "^0.1.3",
    "lodash-webpack-plugin": "^0.10.6",
    "postcss": "^5.2.6",
    "postcss-cli": "^2.5.2",
    "postcss-discard-comments": "^2.0.4",
    "postcss-import": "^9.0.0",
    "postcss-prettify": "^0.3.4",
    "postcss-reporter": "^2.0.0",
    "postcss-scss": "^0.4.0",
    "precss": "^1.4.0",
    "webpack": "^2.2.0"
  }
}
